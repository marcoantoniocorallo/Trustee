let plugin pwd = {
  let pass = "abcd" in 

  let fun checkpwd (guess : string) : bool = 
    pass = guess in
  checkpwd
} in

let filter = include <filter> in
// filter pwd ["pippo", "abc", "abcd", "pluto", "paperino"] // cannot exec without unsafe
unsafe filter pwd ["pippo", "abc", "abcd", "pluto", "paperino"]