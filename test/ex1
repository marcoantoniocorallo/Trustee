// Type System prevents data leakage
    let trust pwd = {
      let secret pass = "abcd" in 

      let fun checkpwd (guess : string) : bool = 
        declassify(pass = guess) in

      handle: {checkpwd}
    } in
    let filter = <"filter"> in
    filter pwd.checkpwd ["pippo", "abc", "abcd", "pluto", "paperino"]