(*
let x = 5 in 
assert taint (x <> 5)
*)

let trust pwd = {
  let pass = "abcd" in 
  
  let fun checkpwd (guess : string) : bool = 
    pass = guess in
    handle: {checkpwd}
} in
  
let filter = include <filter> in
let l = filter pwd.checkpwd ["pippo", "abc", "abcd", "pluto", "paperino"] in 
assert taint l
